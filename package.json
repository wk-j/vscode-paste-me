{
    "name": "vscode-paste-me",
    "displayName": "Paste Me",
    "description": "Paste selected text into text editor",
    "version": "0.0.1",
    "publisher": "wk-j",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.pasteMe.showTexts"
    ],
    "main": "./out/src/extension",
    "icon": "images/icon.png",
    "contributes": {
        "commands": [{
            "command": "extension.pasteMe.showTexts",
            "title": "Paste Me: Show Texts"
        }],
        "keybindings": [{
            "command": "extension.pasteMe.showTexts",
            "key": "ctrl+shift+j",
            "mac": "cmd+shift+j"
        }],
        "configuration": {
			"title": "Paste Me configuration.",
			"type": "object",
			"properties": {
				"saveAndRun": {
					"type": "object",
					"properties": {
						"commands": {
							"type": "array"
                        }
                    }
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}