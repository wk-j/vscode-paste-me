{
    "name": "vscode-paste-me",
    "displayName": "Paste Me",
    "description": "Paste text and file content",
    "version": "0.0.6",
    "publisher": "wk-j",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/wk-j/vscode-paste-me"
    },
    "activationEvents": [
        "onCommand:extension.pasteMe.showTexts",
        "onCommand:extension.pasteMe.showFiles",
        "onCommand:extension.pasteMe.executeCommand"
    ],
    "main": "./out/src/extension",
    "icon": "images/profile.png",
    "contributes": {
        "commands": [
            {
                "command": "extension.pasteMe.showTexts",
                "title": "Paste Me: Show Texts"
            }
        ],
        "keybindings": [
            {
                "command": "extension.pasteMe.showTexts",
                "key": "ctrl+shift+j",
                "mac": "cmd+shift+j"
            },
            {
                "command": "extension.pasteMe.showFiles",
                "key": "ctrl+shift+u",
                "mac": "cmd+shift+u"
            },
            {
                "command": "extension.pasteMe.executeCommand",
                "key": "ctrl+shift+n",
                "mac": "cmd+shift+n"
            }
        ],
        "configuration": {
            "title": "Paste Me configuration.",
            "type": "object",
            "properties": {
                "saveAndRun": {
                    "type": "object",
                    "properties": {
                        "commands": {
                            "type": "array"
                        }
                    }
                }
            }
        }
    },
    "scripts": {
        "publish": "vsce publish",
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "mocha": "^2.3.3",
        "typescript": "^2.0.3",
        "vscode": "^1.0.0"
    },
    "dependencies": {
        "copy-paste": "^1.3.0"
    }
}
